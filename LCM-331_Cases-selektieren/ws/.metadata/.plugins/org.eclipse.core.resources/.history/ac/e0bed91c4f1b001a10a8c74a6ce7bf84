SELECT * FROM BSI_DOCUMENT bd WHERE dbms_lob.substr(BD.X_LOCATION_LOB, 1000, 1) = '1239323.pdf'
;

SELECT * FROM BSI_DOCUMENT bd WHERE dbms_lob.substr(BD.X_LOCATION_LOB, 1000, 1) LIKE '%1007837331_20171213%'
;

SELECT 
	BDOC.X_LOCATION_LOB,
	CMC.X_EXT_JOIN_NR, 
	EJJ.EXT_JOIN_NR,
--	CMC.RESPONSIBLE_USER_NR,
	CMC.CASE_FRAME_NR,
	BDOC.ITEM_KEY0_NR,
	BDOC.EVT_OPEN,
	BDOC.EVT_CREATE
FROM BSI_DOCUMENT bdoc
INNER JOIN BSI_COMMUNICATION cmc 
	INNER JOIN BSI_X_EXT_JOIN_JOIN ejj 
	ON EJJ.JOIN_NR = CMC.COMPANY_NR
ON CMC.COMMUNICATION_NR = BDOC.ITEM_KEY0_NR
WHERE EJJ.JOIN_NO = 1011547999
AND CMC.CASE_FRAME_NR = 0
;

SELECT 
	BDOC.X_LOCATION_LOB,
	CMC.X_EXT_JOIN_NR, 
	EJJ.EXT_JOIN_NR,
--	CMC.RESPONSIBLE_USER_NR,
	CMC.CASE_FRAME_NR,
	BDOC.ITEM_KEY0_NR,
	BDOC.EVT_OPEN,
	BDOC.EVT_CREATE
FROM BSI_DOCUMENT bdoc
INNER JOIN BSI_COMMUNICATION cmc 
	INNER JOIN BSI_X_EXT_JOIN_JOIN ejj 
	ON EJJ.JOIN_NR = CMC.COMPANY_NR
ON CMC.COMMUNICATION_NR = BDOC.ITEM_KEY0_NR
WHERE dbms_lob.substr(BDOC.X_LOCATION_LOB, 500, 1) LIKE '%NX01%'
AND CMC.CASE_FRAME_NR = 0
;


-- 320983398

SELECT CMP.COMPANY_NR, CMP.X_COMPLEX_NO FROM BSI_COMPANY cmp WHERE CMP.COMPANY_NO = 1011547999
;
-- 320998209, 17618097

SELECT 
	DOC.ITEM_KEY0_NR,
	DOC.X_LOCATION_LOB
FROM BSI_DOCUMENT doc INNER JOIN BSI_COMMUNICATION cmc ON CMC.COMMUNICATION_NR = DOC.ITEM_KEY0_NR
WHERE CMC.COMPANY_NR = 19061342
;
