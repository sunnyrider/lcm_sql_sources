	SELECT
		CMP.COMPANY_NR,
		CMP.COMPANY_NO,
		EJJ.JOIN_NO,
		CSE.CASE_NO,
		CSE.CHANNEL_UID,
		CSE.CREATE_USER_NR,
		CSE.X_CLOSE_USER_NR,
		CSE.PROCESS_UID,
		CSE.LAST_NAME,
		CSE.FIRST_NAME,
		CSE.PERSON_NR,
		CSE.EVT_START,
		CSE.EVT_END,
		CSE.PHONE_NO,
		CSE.FAX_NO,
		CSE.EMAIL_ADDRESS,
		CMP.NAME1,
		CMP.X_COMPLEX_NO,
		CMC.COMMUNICATION_NR,
		CMC.TEXT_LOB,
		CMC.DISPLAY_NAME,
		CMC.X_EXT_JOIN_NR
--		EJJ.EXT_JOIN_NR
--		CMC.ORIGIN_UID,
--		CMC.EVT_START cmc_evt_start,
--		CMC.CASE_FRAME_NR
--		CMC.RESPONSIBLE_USER_NR
	FROM BSICRM.BSI_CASE cse
		INNER JOIN BSICRM.BSI_COMMUNICATION cmc 
			INNER JOIN BSI_DOCUMENT BDOC
				INNER JOIN BSI_X_EXT_JOIN_JOIN ejj 
					INNER JOIN BSI_COMPANY cmp ON CMP.COMPANY_NR = EJJ.JOIN_NR
				ON EJJ.EXT_JOIN_NR = BDOC.X_EXT_JOIN_NR
			ON BDOC.ITEM_KEY0_NR = CMC.COMMUNICATION_NR
		ON CMC.COMPANY_NR = CSE.X_COMPANY_NR
--	ON CMC.CASE_FRAME_NR = CSE.CASE_FRAME_NR
--	AND CMC.COMPANY_NR = CSE.X_COMPANY_NR
--	INNER JOIN BSICRM.BSI_COMPANY cmp ON CMP.COMPANY_NR = CSE.X_COMPANY_NR
--	INNER JOIN BSI_X_EXT_JOIN_JOIN ejj ON EJJ.JOIN_NO = cmp.COMPANY_NO
--	INNER JOIN BSI_X_EXT_JOIN_JOIN ejj
--		INNER JOIN BSI_COMPANY cmp ON CMP.COMPANY_NR = EJJ.JOIN_NR
--	ON EJJ.JOIN_NR = cmc.COMPANY_NR

	WHERE CSE.STATUS_UID IN (75959)
--	AND CMC.X_EXT_JOIN_NR = EJJ.EXT_JOIN_NR
	AND CSE.X_COMPANY_NR IS NOT NULL
	AND CMP.IS_ACTIVE = 1
	AND CSE.CASE_FRAME_NR <> 0
	AND cse.PROCESS_UID IN (3271703167, 3364612471, 3457763423, 3518482663, 3903314563, 4573005984, 4619699490, 4649764515, 4782164448, 4903308050, 5534726020, 6153389064, 5711045652, 3131025257, 4619700070, 4706762231, 4782164944, 2688964652, 3047541964, 3094668432, 3154736125, 3162135263, 3246632451, 3248098401, 3271705741, 3358401160, 3364611653, 3415639805, 3427645687, 3433668575, 3591522861, 3710251082, 3952232775, 4024149816, 4095429139, 4630654440, 4638986737, 4646089574, 4712866075, 4713705468, 4764468044, 4782163534, 4787447533, 4794588216, 5066602830, 5101763956, 5287849485, 5103195620, 3246631781, 3248098729, 3271705289, 3358401180, 3364611812, 3415640007, 3764640108, 3884749475, 4619691301, 4782163837, 5633686312, 3271705003, 3352157897, 3364612033, 3415708718, 3865386891, 3884749911, 4619691650, 4778221848, 4782164116, 4829182466, 5066602935, 4134383211, 4619699944, 4621318064, 2688964540, 2847386718, 3088753210, 3094668805, 3147379058, 3155112657, 3155114345, 3246633045, 3248098180, 3271701823, 3309920750, 3352157504, 3364245583, 3370649185, 3506391142, 4062705492, 4075411069, 4619690681, 4666159355, 4716064382, 4718185893, 4782162934, 4802857134, 4941756622, 4944311981, 4944317028, 4944318103, 4953220772, 5066602515, 5136868645, 5444427351, 5473362210, 5569325738, 5585549221, 5625231602, 7558734381, 2688964630, 3060265849, 3101858194, 3147378127, 3246997948, 3248098162, 3309921348, 3352157327, 3364611466, 3370648168, 3370660213, 3415639786, 3699823137, 4062706692, 4062708486, 4062712813, 4619691059, 4782163304, 4802856926, 5728609230, 5968070800, 6070610402, 6250380643, 6850542692, 7158342373, 6250378505, 6913132406, 7268911495, 2688965518, 3309921038, 3358401539, 3364612524, 4619699567, 6265265078, 3047542100, 6148086566, 6185609647, 6321752985, 7289764726, 7404701111, 6058918625, 6100817724, 5661727298, 6609779492, 6850542622, 5268361607, 6505974925, 6114808518, 6135719421, 6151750332, 6223405054, 6306026950, 6906208213)
	AND CSE.EVT_START > TO_DATE('31.12.2014', 'dd.mm.yyyy')
	AND CMC.ORIGIN_UID NOT IN (105356, 143891)
	ORDER BY CSE.X_COMPANY_NR, CSE.CASE_NO
;

	SELECT
		CMP.COMPANY_NR,
		CMP.COMPANY_NO,
		EJJ.JOIN_NO,
		CMC.COMMUNICATION_NR,
		bdoc.X_LOCATION_LOB,
		CMP.NAME1,
		CMP.X_COMPLEX_NO,
		CMC.TEXT_LOB,
		CMC.DISPLAY_NAME,
		CMC.X_EXT_JOIN_NR
	FROM BSICRM.BSI_COMMUNICATION cmc 
			INNER JOIN BSI_DOCUMENT BDOC
				INNER JOIN BSI_X_EXT_JOIN_JOIN ejj 
					INNER JOIN BSI_COMPANY cmp ON CMP.COMPANY_NR = EJJ.JOIN_NR
				ON EJJ.EXT_JOIN_NR = BDOC.X_EXT_JOIN_NR
			ON BDOC.ITEM_KEY0_NR = CMC.COMMUNICATION_NR
	WHERE cmc.company_nr IS NOT NULL
	AND CMP.IS_ACTIVE = 1
	AND CMC.ORIGIN_UID NOT IN (105356, 143891)
	ORDER BY cmc.company_nr, CMC.COMMUNICATION_NR
;


WITH pre_selection AS
(
	SELECT
		CMP.COMPANY_NR,
		CMP.COMPANY_NO,
		EJJ.JOIN_NO,
		CMC.COMMUNICATION_NR,
		bdoc.X_LOCATION_LOB,
		CMC.CASE_FRAME_NR,
		CMP.NAME1,
		CMP.X_COMPLEX_NO,
		CMC.TEXT_LOB,
		CMC.DISPLAY_NAME,
		CMC.X_EXT_JOIN_NR
	FROM BSICRM.BSI_COMMUNICATION cmc 
			INNER JOIN BSI_DOCUMENT BDOC
				INNER JOIN BSI_X_EXT_JOIN_JOIN ejj 
					INNER JOIN BSI_COMPANY cmp ON CMP.COMPANY_NR = EJJ.JOIN_NR
				ON EJJ.EXT_JOIN_NR = BDOC.X_EXT_JOIN_NR
			ON BDOC.ITEM_KEY0_NR = CMC.COMMUNICATION_NR
	WHERE cmc.company_nr IS NOT NULL
	AND CMP.IS_ACTIVE = 1
	AND CMC.ORIGIN_UID NOT IN (105356, 143891)
	ORDER BY cmc.company_nr, CMC.COMMUNICATION_NR
)
SELECT
	CSE.CASE_NR,
	CSE.CASE_NO,
	PSEL.CASE_FRAME_NR,
	PSEL.JOIN_NO,
	PSEL.COMMUNICATION_NR,
	PSEL.X_LOCATION_LOB
FROM pre_selection psel INNER JOIN BSI_CASE cse ON CSE.CASE_FRAME_NR = PSEL.CASE_FRAME_NR
;



SELECT 
	COUNT(BDOC.ITEM_KEY0_NR) anzahl,
	BDOC.ITEM_KEY0_NR
FROM BSI_DOCUMENT bdoc
GROUP BY BDOC.ITEM_KEY0_NR
HAVING COUNT(BDOC.ITEM_KEY0_NR) < 2
;

SELECT 
	BDOC.DOCUMENT_NR,
	BDOC.ITEM_KEY0_NR,
	BDOC.X_LOCATION_LOB
--	BDOC.DOCUMENT_TYPE_UID,
--	BDOC.ITEM_TYPE_ID,
--	BDOC.X_INTERFACE_UID
FROM BSI_DOCUMENT bdoc 
WHERE BDOC.ITEM_KEY0_NR IN (7881515611)
ORDER BY BDOC.ITEM_KEY0_NR, BDOC.DOCUMENT_NR
;
-- = 1
-- 19061340,19061341,19061342,19061343,19061345,19061347,19061348,19061349,19061351,19061353
-- > 1
-- 19061385,19061401,19061411,19061418,19061424,19061427,19061473

SELECT
	count(*) anzahl
FROM BSI_CASE bcs 
INNER JOIN BSI_COMMUNICATION cmc 
--ON CMC.CASE_FRAME_NR = BCS.CASE_FRAME_NR
ON CMC.COMPANY_NR = BCS.X_COMPANY_NR
WHERE CMC.COMMUNICATION_NR = 70046172
;




-- SUCCESS
DECLARE
	l_text varchar(32767) := NULL;
	total_count int := 0;
	added_count int := 0;
BEGIN
	FOR cur_rec IN (
		SELECT DOC.X_LOCATION_LOB FROM BSI_DOCUMENT doc
			WHERE DOC.ITEM_KEY0_NR = 19062485
	) LOOP

	total_count := total_count + 1;

	IF l_text IS NULL THEN
		l_text := cur_rec.X_LOCATION_LOB;
	ELSE
		IF LENGTH(l_text) < 32000 THEN
			l_text := l_text || ', ' || cur_rec.X_LOCATION_LOB;
			added_count := added_count + 1;
		END IF;
	END IF;
	END LOOP;

	IF added_count < total_count THEN
		l_text := l_text || ' .. ' || added_count || ' Links added from Total ' || total_count || '.';
	END IF;

	dbms_output.put_line(l_text);
END;

SELECT 
--	count(*) anzahl 
	BD.DOCUMENT_NR,
	BD.X_LOCATION_LOB
FROM BSI_DOCUMENT bd 
--WHERE BD.X_LOCATION_LOB LIKE '%SG1473P%'
WHERE DOCUMENT_NR = 2340563806
;

UPDATE BSI_DOCUMENT SET X_LOCATION_LOB = REPLACE(dbms_lob.substr(BD.X_LOCATION_LOB, 1000, 1), 'SG1473P', 'NX01') 
WHERE DOCUMENT_NR = 2340563806
;
