SELECT 
	COL.COLLECTION_NR,
	COL.STATUS_UID,
	uc_status.TEXT Status,
	COL.AGREEMENT_UID,
	uc_agm.TEXT Agreement,
	COL.COLLECTION_COMPANY_UID,
	uc_collcmp.TEXT Collection_Company
FROM BSI_X_COLLECTION col
LEFT JOIN BSI_UC_TEXT uc_status ON uc_status.UC_UID = COL.STATUS_UID AND UC_status.LANGUAGE_UID = '246'
INNER JOIN BSI_UC_TEXT uc_agm ON uc_agm.UC_UID = COL.AGREEMENT_UID AND uc_agm.LANGUAGE_UID = '246'
INNER JOIN BSI_UC_TEXT uc_collcmp ON uc_collcmp.UC_UID = COL.COLLECTION_COMPANY_UID AND uc_collcmp.LANGUAGE_UID = '246'
WHERE COL.STATUS_UID NOT IN (138181)
AND COL.AGREEMENT_UID IN (4928209628, 4928209629)
AND COL.COLLECTION_COMPANY_UID = 138179
;

SELECT
	COL.COLLECTION_NR udate_key
FROM BSI_X_COLLECTION col
WHERE COL.STATUS_UID NOT IN (138181)
AND COL.AGREEMENT_UID IN (4928209628, 4928209629)
AND COL.COLLECTION_COMPANY_UID = 138179
;

SELECT 
	COL.COLLECTION_NR,
	COL.STATUS_UID,
	uc_status.TEXT Status,
	COL.AGREEMENT_UID,
	uc_agm.TEXT Agreement,
	COL.COLLECTION_COMPANY_UID
FROM BSI_X_COLLECTION col
LEFT JOIN BSI_UC_TEXT uc_status ON uc_status.UC_UID = COL.STATUS_UID AND UC_status.LANGUAGE_UID = '246'
INNER JOIN BSI_UC_TEXT uc_agm ON uc_agm.UC_UID = COL.AGREEMENT_UID AND uc_agm.LANGUAGE_UID = '246'
WHERE COL.STATUS_UID NOT IN (138150)
AND COL.AGREEMENT_UID IN (4928209628, 4928209629)
AND COL.COLLECTION_COMPANY_UID = 0
;

SELECT
	COL.COLLECTION_NR udate_key
FROM BSI_X_COLLECTION col
WHERE COL.STATUS_UID NOT IN (138181)
AND COL.AGREEMENT_UID IN (4928209628, 4928209629)
AND COL.COLLECTION_COMPANY_UID = 0
;

SELECT
	UCT.UC_UID, UCT.TEXT, UC.CODE_TYPE
FROM BSI_UC uc INNER JOIN BSI_UC_TEXT uct ON UCT.UC_UID = UC.UC_UID 
WHERE UCT.UC_UID IN (
	SELECT DISTINCT COL.STATUS_UID 
	FROM BSI_X_COLLECTION col 
)
AND UCT.LANGUAGE_UID = '246'
;

SELECT * FROM BSI_X_COLLECTION col WHERE COL.COLLECTION_COMPANY_UID = 0
;