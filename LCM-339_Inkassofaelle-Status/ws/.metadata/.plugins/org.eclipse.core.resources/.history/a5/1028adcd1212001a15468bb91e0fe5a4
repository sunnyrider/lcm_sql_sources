SELECT DISTINCT COL.STATUS_UID FROM BSI_X_COLLECTION col WHERE COL.STATUS_UID != 0 
;

SELECT
	UCT.UC_UID, UCT.TEXT, UC.CODE_TYPE
FROM BSI_UC uc INNER JOIN BSI_UC_TEXT uct ON UCT.UC_UID = UC.UC_UID 
WHERE UCT.UC_UID IN (
	SELECT DISTINCT COL.TYPE_UID 
	FROM BSI_X_COLLECTION col WHERE COL.STATUS_UID != 0
)
AND UCT.LANGUAGE_UID = '246'
;
