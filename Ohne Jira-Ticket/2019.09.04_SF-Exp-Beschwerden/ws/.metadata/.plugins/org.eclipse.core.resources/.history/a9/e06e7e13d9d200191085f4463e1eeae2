SELECT
	CFRM.CASE_FRAME_NR,
	CFRM.CHANNEL_UID channel,
	CFRM.INPUT_CHANNEL_UID channel_in,
	CFRM.PARTITION_UID PARTTION,
	CFRM.STATUS_UID
FROM BSI_CASE_FRAME cfrm
WHERE CFRM.CASE_FRAME_NR = 4937776314
;

SELECT
	CFRM.CASE_FRAME_NR,
	CFRM.CHANNEL_UID channel,
	CFRM.INPUT_CHANNEL_UID channel_in,
	CFRM.PARTITION_UID PARTTION,
	CFRM.STATUS_UID,
	CFITEM.ITEM_KEY0_NR key0,
	CFITEM.ITEM_TYPE_ID typeid,
	CFITEM.IS_MASTER
FROM BSI_CASE_FRAME cfrm
	INNER JOIN BSI_CASE_FRAME_ITEM cfitem
	ON CFITEM.CASE_FRAME_NR = CFRM.CASE_FRAME_NR
WHERE CFRM.CASE_FRAME_NR = 4937776314
;


SELECT CASE_NO CASE_NO,
	UT_PROCESS.TEXT PROZESS,
	U_PROCESS.UC_UID process_ID,
	U_PROCESS.EXT_KEY PROZESS_REFERENZ,
	U_PROCESS.SHORTCUT PROZESS_VERSION,
	DECODE(CSA.ACTION_NR, 0, 'Contact Center', 'Marketing') QUELLE,
	UT_CHANNEL.TEXT KANAL,
	UT_STATUS.TEXT STATUS,
	CPNG.DISPLAY_NAME KAMPAGNE,
	ACN.DISPLAY_NAME AKTION,
	ACN.EVT_START AKTION_START,
	ACN.EVT_END AKTION_ENDE,
	CSA.EVT_START BEGINN_DATUM,
	CSA.EVT_END ENDE_DATUM,
	UT_STEP.TEXT AKTUELLER_SCHRITT,
	CSTEP.EVT_START SCHRITT_START_DATUM,
	CMP.X_COMPLEX_NO Firma_NO,
	CMP.DISPLAY_NAME FIRMA_NAME,
	prs.X_COMPLEX_NO PERSON_NO,
	prs.DISPLAY_NAME PERSON_NAME,
	U_RSP.USERNAME AKTUELLER_VERANTWORTLICHER,
	U_CLS.USERNAME ABSCHLIESSENDER_ANWENDER,
	XLD.LEAD_NO,
	CF_ITEM.CASE_FRAME_NR
FROM BSI_CASE CSA
LEFT JOIN BSI_UC U_PROCESS
	INNER JOIN BSI_UC_TEXT UT_PROCESS 
	ON U_PROCESS.UC_UID = UT_PROCESS.UC_UID 
	AND UT_PROCESS.LANGUAGE_UID = 246
ON CSA.PROCESS_UID = U_PROCESS.UC_UID
LEFT JOIN BSI_UC_TEXT UT_CHANNEL ON CSA.CHANNEL_UID = UT_CHANNEL.UC_UID AND UT_CHANNEL.LANGUAGE_UID = 246
LEFT JOIN BSI_UC_TEXT UT_STATUS ON CSA.STATUS_UID = UT_STATUS.UC_UID AND UT_STATUS.LANGUAGE_UID = 246
LEFT JOIN BSI_CASE_STEP CSTEP ON CSA.CASE_NR = CSTEP.CASE_NR AND CSA.CURRENT_CASE_STEP_NR = CSTEP.CASE_STEP_NR
LEFT JOIN BSI_UC_TEXT UT_STEP ON CSTEP.STEP_UID = UT_STEP.UC_UID AND UT_STEP.LANGUAGE_UID = 246
LEFT JOIN BSI_PERSON prs ON CSA.PERSON_NR = prs.PERSON_NR
LEFT JOIN BSI_COMPANY CMP ON CSA.X_COMPANY_NR = CMP.COMPANY_NR
LEFT JOIN BSI_ACTION ACN ON ACN.ACTION_NR = CSA.ACTION_NR
LEFT JOIN BSI_CAMPAIGN CPNG ON CPNG.CAMPAIGN_NR = ACN.CAMPAIGN_NR
LEFT JOIN BSI_USER U_RSP ON CSTEP.RESPONSIBLE_USER_NR = U_RSP.USER_NR
LEFT JOIN BSI_USER U_CLS ON CSA.X_CLOSE_USER_NR = U_CLS.USER_NR
LEFT OUTER JOIN BSI_CASE_FRAME_ITEM CF_ITEM ON CSA.CASE_FRAME_NR=CF_ITEM.CASE_FRAME_NR and CF_ITEM.IS_MASTER=1 AND CF_ITEM.ITEM_TYPE_ID=318597
LEFT OUTER JOIN BSI_X_LEAD XLD ON XLD.ENTRY_COMMUNICATION_NR = CF_ITEM.ITEM_KEY0_NR
--WHERE CF_ITEM.CASE_FRAME_NR = 4946658952
WHERE CSA.STATUS_UID = 75959 -- "Erledigt"
;
 
