SELECT
	CFRM.CASE_FRAME_NR,
	CFRM.CHANNEL_UID channel,
	CFRM.INPUT_CHANNEL_UID channel_in,
	CFRM.PARTITION_UID PARTTION,
	CFRM.STATUS_UID
FROM BSI_CASE_FRAME cfrm
WHERE CFRM.CASE_FRAME_NR = 4937776314
;

SELECT
	CFRM.CASE_FRAME_NR,
	CFRM.CHANNEL_UID channel,
	CFRM.INPUT_CHANNEL_UID channel_in,
	CFRM.PARTITION_UID PARTTION,
	CFRM.STATUS_UID,
	CFITEM.ITEM_KEY0_NR key0,
	CFITEM.ITEM_TYPE_ID typeid,
	CFITEM.IS_MASTER
FROM BSI_CASE_FRAME cfrm
	INNER JOIN BSI_CASE_FRAME_ITEM cfitem
	ON CFITEM.CASE_FRAME_NR = CFRM.CASE_FRAME_NR
WHERE CFRM.CASE_FRAME_NR = 4937776314
;