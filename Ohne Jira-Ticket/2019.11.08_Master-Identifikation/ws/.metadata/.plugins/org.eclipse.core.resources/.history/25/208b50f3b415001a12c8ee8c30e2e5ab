;

SELECT * FROM INTF_LOG_VIEW ilv WHERE ILV.EVT_LOG > TO_DATE('03.12.2019 02:25', 'DD.MM.YYYY HH24:MI')
ORDER BY ILV.EVT_LOG
;

SELECT
	CP.PERSON_NR,
	SUBSTR(LISTAGG(c.display_name , ', ') within 
			GROUP(ORDER BY c.display_name), 0, 250
		) v_company_names
	FROM bsi_company c JOIN bsi_company_person cp ON c.company_nr = cp.company_nr
	AND c.DISPLAY_NAME NOT IN ('SWISSCOM DIRECTORIES AG, ZÜRICH')
--	AND cp.person_nr = '1005047'
	WHERE cp.role_uid IN (
		SELECT company_person_role_uid FROM bsi_uc_company_person_role uccpr
		WHERE uccpr.is_use_for_display_name = 1)
	GROUP BY cp.PERSON_NR
	ORDER BY cp.PERSON_NR
;