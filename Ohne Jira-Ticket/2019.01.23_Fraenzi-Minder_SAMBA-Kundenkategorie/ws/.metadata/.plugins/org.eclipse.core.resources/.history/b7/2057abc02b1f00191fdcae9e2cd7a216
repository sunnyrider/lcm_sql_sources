SELECT * FROM BSI_COMPANY;

SELECT uct.text, UC.* FROM BSI_UC uc
INNER JOIN BSI_UC_TEXT uct
ON UCT.UC_UID = UC.UC_UID
WHERE lower(UCT.TEXT) LIKE 'key%account%'
AND UCT.LANGUAGE_UID = 246;

SELECT uct.text, UC.* FROM BSI_UC uc
INNER JOIN BSI_UC_TEXT uct
ON UCT.UC_UID = UC.UC_UID
WHERE UC.CODE_TYPE = 143811
AND UCT.LANGUAGE_UID = 246;
